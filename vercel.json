{
  "builds": [
    {
      "src": "Testing_project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": ".vercel/output/static/$1" },
    { "src": "/media/(.*)", "dest": ".vercel/output/media/$1" },
    { "src": "/(.*)", "dest": "Testing_project/wsgi.py" }
  ]
}
